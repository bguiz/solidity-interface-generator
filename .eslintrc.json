{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "prettier/prettier": ["error"],
      "import/extensions": [2, "ignorePackages"],
      "import/no-unresolved": 2,
      "import/no-commonjs": 2,
      "no-underscore-dangle": "off"
    }
}
